{
  "name": "@hashicorp/docs-preview",
  "version": "0.0.1-docs-preview.b42b6ab6b678a80b755ce6248838e2c47b246360.0",
  "bin": "bin/index.sh",
  "scripts": {
    "dev": "npm run start",
    "dev-next-app": "next-remote-watch ./content/**/*.mdx",
    "dev-watch-product-files": "node ./scripts/watch-product-files",
    "build": "node --max-old-space-size=2048 ./node_modules/.bin/next build",
    "export": "node --max-old-space-size=2048 ./node_modules/.bin/next export",
    "format": "next-hashicorp format",
    "generate:component": "next-hashicorp generate component",
    "generate:readme": "next-hashicorp markdown-blocks README.md",
    "lint": "next-hashicorp lint",
    "start": "concurrently -n site,content \"npm run dev-next-app\" \"npm run dev-watch-product-files\"",
    "static": "npm run build && npm run export"
  },
  "dependencies": {
    "@hashicorp/mktg-logos": "^1.2.0",
    "@hashicorp/platform-code-highlighting": "^0.1.1",
    "@hashicorp/platform-util": "^0.1.0",
    "@hashicorp/react-docs-page": "^14.0.3",
    "@hashicorp/react-min-100-layout": "^2.0.1",
    "@hashicorp/react-product-downloads-page": "^2.5.2",
    "@hashicorp/react-section-header": "^5.0.4",
    "@hashicorp/react-subnav": "^9.0.1",
    "@hashicorp/react-tabs": "^7.0.1",
    "@hashicorp/react-vertical-text-block-list": "^7.0.0",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@hashicorp/platform-cli": "^1.1.2",
    "@hashicorp/platform-nextjs-plugin": "^0.3.0",
    "@hashicorp/platform-types": "^0.1.1",
    "@types/react": "17.0.20",
    "chokidar": "^3.5.2",
    "concurrently": "^6.2.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "next-remote-watch": "^1.0.0",
    "typescript": "4.4.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hashicorp/dev-portal.git"
  },
  "author": "Jeff Escalante",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/hashicorp/dev-portal/issues"
  },
  "homepage": "https://github.com/hashicorp/dev-portal#readme"
}
