			"begin": "\"",
			"end": "\"",
			"name": "string.quoted.double.untitled"
		},
		{
			"name": "support.function.builtin.sentinel",
			"match": "\\b(append|delete|error|keys|length|print|range|values|int|float|string|bool)\\b"
		}
